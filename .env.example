# Discord Bot Token
DISCORD_TOKEN=your_discord_bot_token

# Character.AI API Tokens (you can add multiple for rotation)
CHARACTERAI_TOKEN=your_characterai_token
CHARACTERAI_TOKEN_2=your_second_characterai_token
CHARACTERAI_TOKEN_3=your_third_characterai_token

# Character.AI Character ID
CHARACTERAI_CHARACTER_ID=your_characterai_character_id

# Character.AI Persistent Chat ID
# IMPORTANT: Set this to use a persistent chat ID instead of creating new chats
# To get a chat ID:
# 1. Create a chat with your character on Character.AI website
# 2. Check the URL, it will look like: https://character.ai/chat?char=CHAR_ID&hist=CHAT_ID
# 3. Copy the CHAT_ID part after &hist= and paste it below
CHARACTERAI_CHAT_ID=your_persistent_chat_id

# Other API Keys
GEMINI_API_KEY=your_gemini_api_key
GROQ_API_KEY=your_groq_api_key
CEREBRAS_API_KEY=your_cerebras_api_key
TOGETHER_API_KEY=your_together_api_key
DEEPSEEK_API_KEY=your_deepseek_api_key
CHATGPT_API_KEY=your_chatgpt_api_key

# GitHub configuration for saving channel preferences
GITHUB_TOKEN=your_github_token
GITHUB_REPO=username/repo

# Bot owner ID (Discord user ID)
BOT_OWNER_ID=your_discord_user_id

# YouTube API Key (for YouTube video features)
YOUTUBE_API_KEY=your_youtube_api_key 